{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "EnterpriseMcpIntegration": "Debug"
    }
  },
  "AllowedHosts": "*",
  "MicrosoftDocs": {
    "EndpointUrl": "https://learn.microsoft.com/api/mcp",
    "EnableCaching": true,
    "CacheExpiryMinutes": 30,
    "Timeout": "00:05:00",
    "MaxRetryAttempts": 3
  },
  "Enterprise": {
    "ServiceName": "Enterprise MCP Integration API",
    "Version": "1.0.0",
    "Environment": "Development"
  }
}
